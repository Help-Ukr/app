{
    "name": "donations-management-app",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "dev": "tsc && next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "gen-api": "./scripts/generate-api.sh",
        "gen-tr": "yarn --silent ts-node -P tsconfig.scripts.json scripts/generate-translations.ts > texts/_$LANG.ts; mv -f texts/_$LANG.ts texts/$LANG.ts",
        "gen-tr-uk": "LANG=uk yarn --silent gen-tr",
        "gen-tr-de": "LANG=de yarn --silent gen-tr",
        "docker": "docker build --pull . -t registry.softdrive.com.ua/donation && read && docker push registry.softdrive.com.ua/donation"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.6.0",
        "@mui/material": "^5.6.0",
        "@uk/log": "^2.1.0",
        "browser-image-compression": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "geolib": "^3.3.3",
        "i18next": "^21.6.14",
        "leaflet": "^1.7.1",
        "mobx": "^6.5.0",
        "mobx-react-lite": "^3.3.0",
        "next": "12.1.4",
        "next-auth": "^4.3.1",
        "query-string": "^7.1.1",
        "react": "^17",
        "react-dom": "^17",
        "react-i18next": "^11.16.2",
        "react-images-uploading": "^3.1.3",
        "react-leaflet": "^3.2.5",
