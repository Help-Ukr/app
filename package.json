        "typedi": "^0.10.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.1.4",
        "@types/leaflet": "^1.7.9",
        "@types/node": "^16",
        "@types/react": "^17",
        "eslint": "8.12.0",
        "eslint-config-next": "12.1.4",
        "next-compose-plugins": "^2.2.1",
        "openapi-typescript": "^5.2.0",
        "ts-node": "^10.7.0",
        "ts-toolbelt": "^9.6.0",
        "typescript": "4.6.3"
    },
    "resolutions": {
        "@types/react": "^17"
    }
